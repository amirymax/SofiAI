{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-var": "off",
        "require-jsdoc": "off",
        "camelcase": "off",
        "no-unused-vars": "off",
        "max-len": ["error", {"code": 150}]
    }
}
