{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": "eslint-config-standard",
    "parserOptions": {
      "ecmaVersion": "latest"
    },
    "ignorePatterns": ["node_modules/"],
    "rules": {
      "operator-linebreak": ["error", "none"],
      "n/handle-callback-err": "off",
      "no-unused-vars": "off",
      "camelcase": "off" ,
      "no-undef": "off"
    }
  }
  